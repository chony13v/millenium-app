{
  "expo": {
    "name": "millenium-app",
    "slug": "millenium-app",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "millenium-app",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "runtimeVersion": "1.0.0+firebase-test",
    "owner": "chony13v",

    "ios": {
      "supportsTablet": true
    },

    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.chony.milleniumapp",
      "versionCode": 1,
      "permissions": [],
      "googleServicesFile": "./android/app/google-services.json"
    },

    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },

    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/splash-icon.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff"
        }
      ],
      [
        "expo-build-properties",
        {
          "android": {
            "googleServicesFile": "./android/app/google-services.json"
          }
        }
      ],
      [
        "expo-notifications",
        {
          "icon": "./assets/images/icon.png",
          "color": "#ffffff",
          "sounds": ["default"],
          "useNextNotificationsApi": true
        }
      ]
    ],

    "experiments": {
      "typedRoutes": true
    },

    "extra": {
      "router": {
        "origin": false
      },
      "EXPO_PUBLIC_CLERK_PUBLISHABLE_KEY": "pk_test_bWFzc2l2ZS1idW5ueS0zMi5jbGVyay5hY2NvdW50cy5kZXYk",
      "eas": {
        "projectId": "d0bb8c11-9d37-4e83-9fcc-f057ffe6563c"
      }
    }
  }
}
